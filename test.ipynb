    "function isPrime(n)\n",
    "    if n < 2\n",
    "        return false\n",
    "    elseif n == 2 || n == 3\n",
    "        return true\n",
    "    else\n",
    "        for i = 2:n-1\n",
    "            if n % i == 0\n",
    "                return false\n",
    "            end\n",
    "        end\n",
    "        return true\n",
    "    end\n",
    "end"
   ]
  }
